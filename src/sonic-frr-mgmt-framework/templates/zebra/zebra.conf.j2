{% if EVPN_MH_GLOBAL is defined %}
! EVPN MH global config
{%  if 'startup_delay' in EVPN_MH_GLOBAL['default'] %}
evpn mh startup-delay {{EVPN_MH_GLOBAL['default']['startup_delay']}}
{%  endif %}
{%  if 'mac_holdtime' in EVPN_MH_GLOBAL['default'] %}
evpn mh mac-holdtime {{EVPN_MH_GLOBAL['default']['mac_holdtime']}}
{%  endif %}
{%  if 'neigh_holdtime' in EVPN_MH_GLOBAL['default'] %}
evpn mh neigh-holdtime {{EVPN_MH_GLOBAL['default']['neigh_holdtime']}}
{%  endif %}
{% endif %}
